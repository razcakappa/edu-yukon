<?php

namespace EDUBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * UsersTokens
 *
 * @ORM\Table(name="users_tokens")
 * @ORM\Entity(repositoryClass="EDUBundle\Repository\UsersTokensRepository")
 */
class UsersTokens
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="token", type="string")
     */
    private $token;

    /**
     * Get id
     *
     * @return int
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set token
     *
     * @param string $token
     *
     * @return UsersTokens
     */
    public function setToken($token)
    {
        $this->token = $token;

        return $this;
    }

    /**
     * Get token
     *
     * @return string
     */
    public function getToken()
    {
        return $this->token;
    }
}
